<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.rushi.e_agree.Navigation_page"
    tools:showIn="@layout/app_bar_navigation_page">

    <ImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/buy_nav"
        android:id="@+id/buyImage"
        android:onClick="buy"
        />

    <ImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/sell_nav"
        android:id="@+id/sellImage"
        android:onClick="sell"
        />

</RelativeLayout>
